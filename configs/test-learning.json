{
  "scenario": "4x4,1v1",
  "time_limit": 100,

  "state_representation": "coordinates",

  "algorithm": "sac",
  "encoder": "fc",

  "encoder_layer_size": 32,
  "num_encoder_layers": 2,
  "activation_function": "tanh",

  "num_iterations": 50,
  "memory_size": 10000,
  "num_transitions": 1000,
  "batch_size": 256,
  "num_epochs": 8,

  "entropy_coef_milestones":   [   0],
  "entropy_coef_values":       [0.01],

  "lr_milestones":             [0],
  "lr_values":                 [0.001],

  "uniform_proba_milestones": [0, 10],
  "uniform_proba_values":     [1, 0],

  "win_rate_threshold": 1.0,

  "discount": 0.97,
  "max_grad_norm": 10,

  "log_interval": 1,
  "eval_interval": 999,
  "save_interval": 999,

  "seed": 0
}
