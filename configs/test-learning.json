{
  "scenario": "6x6,2v2",
  "time_limit": 100,
  "allow_noop": [true, true],
  "allow_diagonals": [false, false],

  "algorithm": "pg",
  "controller": "conv",

  "hidden_layer_size": 64,
  "num_hidden_layers": 3,
  "activation_function": "relu",
  "num_recurrent_layers": 0,

  "num_iterations": 50,
  "num_transitions": 4000,
  "batch_size": 512,
  "num_epochs": 2,

  "entropy_coef_milestones":   [   0],
  "entropy_coef_values":       [0.01],

  "uniform_proba_milestones":  [0],
  "uniform_proba_values":      [0],

  "scripted_proba_milestones": [0],
  "scripted_proba_values":     [1],

  "lr_milestones":             [0],
  "lr_values":                 [0.001],

  "inverse_proba_milestones":  [0],
  "inverse_proba_values":      [0],


  "discount": 0.99,
  "max_grad_norm": 10,

  "log_interval": 1,
  "eval_interval": 5,
  "save_interval": 0,

  "seed": 0
}
